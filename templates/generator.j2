    {% endfor %}
};

static uint8_t GENERATORS_MEDIUM[{{len(sc_medium_gen)}}][{{ max_gens_medium }}] = {
    {% for generators in sc_medium_gen %}
    { {{ generators|join(', ') }} },
    {% endfor %}
};

#endif

